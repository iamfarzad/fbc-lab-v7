{
  "name": "fb-consulting-websocket-server",
  "version": "1.0.0",
  "description": "WebSocket server for real-time voice and multimodal AI interactions",
  "type": "module",
  "main": "live-server.js",
  "scripts": {
    "start": "node dist/live-server.js",
    "dev": "tsx --env-file=.env.local live-server.ts",
    "build": "tsc -p tsconfig.json",
    "deploy": "sh deploy-fly.sh",
    "logs": "fly logs",
    "status": "fly status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@google/genai": "^1.21.0",
    "dotenv": "^17.2.1",
    "tsx": "^4.20.3",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.17.0"
  }
}
